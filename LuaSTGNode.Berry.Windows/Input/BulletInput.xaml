<local:InputWindow x:Name="inputWindow"  x:Class="LuaSTGEditorSharp.Windows.Input.BulletInput"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LuaSTGEditorSharp.Windows.Input;assembly=LuaSTGEditorSharp.Core.Windows"
        mc:Ignorable="d"
        Title="Bullet Style Selection" Height="720.818" Width="320" 
                   Loaded="InputWindow_Loaded">
    <local:InputWindow.Style>
        <Style TargetType="Window" BasedOn="{StaticResource {x:Type Window}}"/>
    </local:InputWindow.Style>
    <Window.Resources>
        <Style x:Key="SelectionImg" TargetType="Image">
            <Setter Property="HorizontalAlignment" Value="Stretch"/>
            <Setter Property="VerticalAlignment" Value="Stretch"/>
            <Setter Property="LayoutTransform">
                <Setter.Value>
                    <TransformGroup>
                        <ScaleTransform ScaleX="0.75" ScaleY="0.75" />
                    </TransformGroup>
                </Setter.Value>
            </Setter>
            <Setter Property="Stretch" Value="None"/>
            <Setter Property="Width" Value="96"/>
            <Setter Property="Height" Value="96"/>
        </Style>
    </Window.Resources>
    <WrapPanel ScrollViewer.VerticalScrollBarVisibility="Auto"  ScrollViewer.CanContentScroll="True">
        <Button Click="Style_Click" Tag="arrow_big" Padding="0">
            <Button.Content>
                <Image Source="/LuaSTGNode.Legacy;component/images/bullet/scale.png" Style="{StaticResource SelectionImg}"/>
            </Button.Content>
        </Button>
        <Button Click="Style_Click" Tag="arrow_mid" Padding="0">
            <Button.Content>
                <Image Source="/LuaSTGNode.Legacy;component/images/bullet/arrow.png" Style="{StaticResource SelectionImg}"/>
            </Button.Content>
        </Button>
        <Button Click="Style_Click" Tag="arrow_small" Padding="0">
            <Button.Content>
                <Image Source="/LuaSTGNode.Legacy;component/images/bullet/chain.png" Style="{StaticResource SelectionImg}"/>
            </Button.Content>
        </Button>
        <Button Click="Style_Click" Tag="gun_bullet" Padding="0">
            <Button.Content>
                <Image Source="/LuaSTGNode.Legacy;component/images/bullet/bullet.png" Style="{StaticResource SelectionImg}"/>
